{
  "inputFiles": [
	"./src/"
  ],
  "mode": "modules",
  "exclude": [
	"**/__test__/**/*",
	"**/__node_modules__/**/*"
  ],
  "includeDeclarations": true,
  "externalPattern": "**/node_modules/**",
  "excludeExternals": true,
  "excludeNotExported": true,
  "tsconfig": "./tsconfig.json",
  "out": "../../../typedocs/bft",
  "ignoreCompilerErrors": true,
  "plugin": "none",
  "readme": "none",
  "theme": "../../../lisk-docs-typedoc-theme"
}
