{
  "inputFiles": [
	"./src/index.ts"
  ],
  "mode": "file",
  "exclude": [
	"**/__test__/**/*",
	"**/__node_modules__/**/*"
  ],
  "includeDeclarations": true,
  "externalPattern": "**/node_modules/**",
  "excludeExternals": true,
  "excludeNotExported": true,
  "tsconfig": "./tsconfig.json",
  "out": "../../typedocs/validator",
  "ignoreCompilerErrors": true,
  "plugin": "none",
  "readme": "none",
  "theme": "../../../lisk-docs-typedoc-theme"
}
